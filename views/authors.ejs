<%- include('./includes/head.ejs')%> 
</head>
<body>
<%- include('./includes/nav.ejs')%>
<div>
  <% if(authors.length>0) {%>
    <br>
  <table>
    <tr>
      <th>Email</th>
      <th>First Name</th>
      <th>Last Name</th>
    </tr>
    <% for(let author in authors){%>
    <tr>
      <td><%=authors[author].email%></td>
      <td><%=authors[author].firstname%></td>
      <td><%=authors[author].lastname%></td>
    </tr>
    <%}%> <%} else{%>
  </table>
  <h1>No Authors found</h1>
  <%}%>
</div>
<%- include('./includes/end.ejs')%>